block logo {

    !this.mods.lang, !this.elem, mix: [
        { mods: { lang: 'ru' } }
    ]

    !this.elem, ~['en', 'tr'].indexOf(this['i-global'].lang) {

        mix: [{ mods: { lang: 'en' } }]

    }

    !this.elem, content: [
        {
            elem: 'link',
            url: this.ctx.url,
            content: {
                elem: 'image',
                alt: this.ctx.alt
            }
        },
        this.ctx.content
    ]

    elem link {
        tag: 'a'
        attrs: { href: this.ctx.url || this['i-services'].serviceUrl('www') }
    }

    elem image {
        tag: 'img',
        attrs: {
            alt:  this.ctx.alt || BEM && BEM.I18N('logo', 'yandex'),
            src: '//yastatic.net/lego/_/La6qi18Z8LwgnZdsAr1qy1GwCwo.gif'
        }
    }

}
